{
  "name": "telemetry",
  "author": "RCKT Software, LLC.",
  "description": "An open-source progress tracker that predicts when you'll reach your goals",
  "private": true,
  "version": "0.3.49",
  "main": "main.js",
  "scripts": {
    "start": "npm run build && npm run electron:start",
    "dev": "concurrently -k \"vite\" \"npm run electron:dev\"",
    "dev-vite": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "cross-env IS_DEV=true electron-forge start",
    "electron:build": "npm run build && electron-forge make",
    "electron:package": "electron-forge package"
  },
  "dependencies": {
    "@kurkle/color": "^0.3.2",
    "@vueuse/core": "^10.5.0",
    "cannon-es": "^0.19.0",
    "cannon-es-debugger": "^1.0.0",
    "chart.js": "^4.4.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "sass": "^1.69.4",
    "sequelize": "^6.35.0",
    "sqlite3": "^5.1.6",
    "sugar": "^2.0.6",
    "systeminformation": "^5.21.14",
    "uuid": "^9.0.1",
    "vue": "^3.3.7"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "@vitejs/plugin-vue": "^2.3.3",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "electron": "^19.0.8",
    "vite": "^2.9.9"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "appCopyright": "Copyright © 2023 RCKT Software, LLC.",
        "icon": "./src/assets/icon",
        "name": "Telemetry",
        "win32metadata": {
          "ProductName": "Telemetry"
        }
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Telemetry",
            "copyright": "Copyright © 2023 RCKT Software, LLC.",
            "iconUrl": "https://raw.githubusercontent.com/RCKT-Software/telemetry/main/public/icon.ico",
            "setupIcon": "./src/assets/icon.ico"
          }
        }
      ]
    }
  }
}
